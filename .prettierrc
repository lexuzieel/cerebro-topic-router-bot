{
    "tabWidth": 4,
    "quoteProps": "as-needed",
    "singleQuote": false,
    "printWidth": 80,
    "semi": true,
    "useTabs": false,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "plugins": [
        "@ianvs/prettier-plugin-sort-imports"
    ],
    "importOrder": [
        "^@core/(.*)$",
        "^@server/(.*)$",
        "^@ui/(.*)$",
        "^[./]"
    ],
    "importOrderParserPlugins": [
        "typescript",
        "decorators-legacy"
    ]
}